<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gio.gpsapp">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="GPS App"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ClosableActivity" />
        <activity android:name=".Location1" />
        <activity android:name=".Location2" />
        <activity android:name=".Location3" />
        <activity android:name=".Location4" />
        <activity android:name=".Location5" />
        <activity android:name=".Location6" />
        <activity android:name=".Location15" />
        <activity android:name=".Location25" />
        <activity android:name=".Location35" />
        <activity android:name=".Location45" />
        <activity android:name=".Location55" />
        <activity android:name=".Location65" />
        <activity android:name=".Location75"></activity>
    </application>

</manifest>